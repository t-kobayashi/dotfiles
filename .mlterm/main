# Meta キーとして使用するキーを指定
# (none,mod1,mod2,mod3,mod4,meta,alt,super,hyper)
mod_meta_key = alt

# Meta キーを押下した時の挙動
# (none,esc,8bit)
mod_meta_mode = esc

# BEL(0x7)を受け取った時の挙動(none,sound,visual,sound|visual)
bel_mode = none

# フォーカスを失った画面上でBEL(0x7)を受け取った時に、ユーザに警告する。
# use_urgent_bell = false

# daemon process となるかどうか(none,blend,genuine)
# daemon_mode = none

# スクロールのショートカットキーとして、PAGE_UP,PAGE_DOWN ショートカット以
# 外に、Up,Down,j,k,Prior,Next キー及び、SCROLL_UP,SCROLL_DOWN ショートカ
# ットを有効にするかどうか
# use_extended_scroll_shortcut = false

# pty をオープンしたときに、端末に送られる初期化文字列を指定する。
# init_str =

# ダブルクリックの間隔(ミリ秒)
# click_interval = 250

# バックスクロール時に端末出力があっても画面が流れないようにする。
# static_backscroll_mode = false

# バックスクロール中に、端末出力があった場合、バックスクロールを抜ける。
# exit_backscroll_by_pty = false

# pty から流れてくる VT sequence を ~/.mlterm/[device].log に記録する。
# logging_vt_seq = false

# ~/.mlterm/[device].log に記録するフォーマットを指定する。(raw,ttyrec)
# vt_seq_format = raw

# 領域選択した文字列をコピーする際に PRIMARY だけでなく CLIPBOARD も使用する。
# use_clipboard = true

# mlterm が出力するメッセージを ~/.mlterm/msg[pid].log に出力する。
# logging_msg = true

# ssh 等により直接ログインするサーバを指定する。
# default_server =

# always_show_dialog = false

# ssh 接続の認証で使用する公開鍵のファイルを指定する。
# ssh_public_key =

# ssh 接続の認証で使用する秘密鍵のファイルを指定する。
# ssh_private_key =

# ssh 接続で使用する共通鍵暗号を指定する。
# cipher_list =

# ssh 接続中に、指定した秒数ごとに、SSH_MSG_IGNORE を送信する。
# ssh_keepalive_interval = 0

# ssh 接続で x11 forwarding を有効にする。
# ssh_x11_forwarding = false

# ssh 接続時に OSC 5379 scp を許可する。
# allow_scp = false

# ssh の接続が予期せず切断した場合に、自動的に再接続を試みる。
# ssh_auto_reconnect = false

# SIGSEGV, SIGBUS, SIGFPE, SIGILL が発生した場合に、mlterm 内部の console
# application (ssh 接続を除く。)を引き継いだ mlterm を起動し直す。
# auto_restart = true

# OSC 52 シーケンスによるクリップボード操作
# allow_osc52 = false

# local echo mode を有効にする。
# use_local_echo = false

# Alternate screen buffer を有効にする。
# use_alt_buffer = true

# ANSI color change escape sequences を有効にする。
# use_ansi_colors = true

# OSC 5379 の "set_shortcut" による動的なショートカットキー定義を有効にする。
# allow_change_shortcut = false

# 子プロセス(シェル)のカレントディレクトリを指定する。
# working_directory =

# ペーストする文字列の末尾の改行を削除する。
# trim_trailing_newline_in_pasting = false

# キー入力及びペーストされた文字列を、全ての pty (ignore_broadcasted_chars オプ
# ションの値が false なものに限る) に送信する。
# broadcast = false

# broadcast オプションが true の場合でも、ブロードキャストを受け付けない。
# ignore_broadcasted_chars = false

# default_server オプションが指定されていない場合に、起動時の SSH 接続ダイ
# アログを表示しないようにする。
# (mlterm-fb on NetBSD/x68k or OpenBSD 用)
# start_with_local_pty = false

# 768x512x4 のような形式で解像度を指定する。
# (mlterm-fb on NetBSD/x68k 用)
# fb_resolution =

# 壁紙のみ Graphic VRAM ではなく Text VRAM に描画し、スクロール測度を改善する。
